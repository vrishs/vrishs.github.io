{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/vrishs/vrishs.github.io/main/ETHUSD6.csv",
      "format": {"type": "csv"}
    },
    "mark": {
      "type": "line",
      "color": "#b74147",
      "strokeWidth": 1.5,
      "opacity": 2
    },
    "title": {
      "text": "Ethereum (ETH) from 2018-2022",
      "subtitle": "Daily ETH price in USD. Source: Coincodex",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 300,
    "height": 300,
    "encoding": {
      "x": {"field": "Date", "type": "temporal", "title": "Date"},
      "y": {"field": "Close", "type": "quantitative", "title": "Price ($}"},
      "tooltip": [
        {"field": "Date", "type": "temporal", "title": "Date"},
        {"field": "Close", "type": "quantitative", "title": "Price ($)"}
      ]
    },
    "config": {}
  }